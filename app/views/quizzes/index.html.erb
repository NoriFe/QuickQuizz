<div class="background-black">
<h2>Quiz List</h2>
</div>
<div class="container">
    <div class="mx-auto">
      <% @quizzes.each do |quiz| %>
        <div class="col-3 my-2">
          <%= link_to(quiz_path(quiz)) do %>
           <div class="d-flex flex-column h-100 rounded-4 bg-dark bg-opacity-50 text-secondary p-3">
             <div><h2 class="card-title mt-3"><%= quiz.title %></h2></div>
             <div class="d-flex flex-column flex-grow-1 align-content-between flex-wrap">
               <p>Results</p>    <%#<%= quiz_results.result %>
             </div>
           </div>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
